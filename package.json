        "autoprefixer": "^10.4.4",
        "chokidar": "^3.5.3",
        "classnames": "^2.3.1",
        "concurrently": "^7.1.0",
        "eslint": "^8.14.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-react": "^7.29.4",
        "eslint-plugin-react-hooks": "^4.4.0",
        "jquery": "^3.6.0",
        "lessc": "^1.0.2",
        "nodemon": "^2.0.15",
        "react": "^18.0.0",
