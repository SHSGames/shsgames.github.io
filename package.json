{
    "name": "photon-pwa",
    "version": "1.0.0",
    "description": "Create single page applications using PhotonCSS",
    "main": "./lib/index.js",
    "scripts": {
        "build": "tsc; ./node_modules/.bin/uuid > hash && ./node_modules/.bin/webpack --config webpack.prod.cjs && mv hash public_html/ && cp public_html/index.html public_html/404.html",
        "watch-ts": "tsc -w --preserveWatchOutput",
        "watch-tsx": "cd src && tsc -w --preserveWatchOutput",
        "watch-js": "env NODE_ENV=dev ./node_modules/.bin/nodemon .",
        "watch-webpack": "./node_modules/.bin/uuid > hash && ./node_modules/.bin/webpack-dev-server --config webpack.dev.cjs",
        "dev": "./node_modules/.bin/concurrently \"npm:watch-*\"",
        "dev-api": "./node_modules/.bin/concurrently \"npm:watch-js\" \"npm:watch-ts\"",
        "serve": "node ."
    },
    "keywords": [],
    "author": "Josh Merlino",
    "license": "ISC",
    "nodemonConfig": {
        "ignore": [
            "src/*"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.12.1",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.9.0",
        "@babel/preset-react": "^7.9.4",
        "@babel/preset-typescript": "^7.13.0",
        "@types/body-parser": "^1.19.0",
        "@types/compression": "^1.7.0",
        "@types/cors": "^2.8.10",
        "@types/express": "^4.17.11",
        "@types/express-rate-limit": "^5.1.1",
        "@types/jquery": "^3.5.5",
        "@types/node": "^14.14.31",
        "@types/react": "^17.0.3",
        "@types/webpack": "^5.28.0",
        "@types/webpack-env": "^1.16.0",
        "@typescript-eslint/eslint-plugin": "^4.17.0",
        "@typescript-eslint/parser": "^4.17.0",
        "app-manifest-webpack-plugin": "^1.2.0",
        "babel-loader": "^8.1.0",
        "classnames": "^2.2.6",
        "clean-webpack-plugin": "^3.0.0",
        "concurrently": "^5.3.0",
        "copy-webpack-plugin": "^6.4.1",
        "css-loader": "^5.0.0",
        "duplicate-package-checker-webpack-plugin": "^3.0.0",
        "eslint": "^7.21.0",
        "eslint-plugin-react": "^7.22.0",
        "file-loader": "^6.2.0",
        "html-webpack-partials-plugin": "^0.7.0",
        "html-webpack-plugin": "^4.5.2",
        "import-local": "^3.0.2",
        "jquery": "^3.5.0",
        "less": "^3.11.1",
        "less-loader": "^7.0.2",
        "mini-css-extract-plugin": "^1.2.1",
        "module-mapping-webpack-plugin": "^1.1.1",
        "nodemon": "^2.0.7",
        "offline-plugin": "^5.0.7",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "photoncss": "^12.16.1",
        "preload-webpack-plugin": "^3.0.0-beta.3",
        "purgecss-webpack-plugin": "^4.0.2",
        "query-string": "^6.13.6",
        "raw-loader": "^4.0.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-router-dom": "^5.1.2",
        "resolve-from": "^5.0.0",
